window.ModernFacebookMessengerAppID = '';
function loadStyle(url) {
    var script = document.createElement('link');
    script.setAttribute("rel", "stylesheet");
    script.setAttribute("type", "text/css");
    script.setAttribute("href", url);
    document.getElementsByTagName('head')[0].appendChild(script);
    return;
}

var Analyzer = {
    timer: null,
    wait_for_move_in_sec: 5000,
    check_focus_timer: null,
    results: {
        current: 0,
        max: 0,
        geo_data: null,
        timezone: null,
        plugins: null,
        screen: null,
        browser: null,
        browser_version: null,
        browser_language: null,
        google_analytics: null
    },
    props: {
        container: window,
        confirm_on_close: false,
        check_focus_interval: 200
    },
    getResults: function () {
        return this.results;
    },
    getScreenData: function () {

        var screen_data = {};
        screen_data.availHeight = screen.availHeight;
        screen_data.availLeft = screen.availLeft;
        screen_data.availTop = screen.availTop;
        screen_data.availWidth = screen.availWidth;
        screen_data.colorDepth = screen.colorDepth;
        screen_data.height = screen.height;
        screen_data.orientation = {
            angle: screen.orientation.angle,
            type: screen.orientation.type
        };
        screen_data.pixelDepth = screen.pixelDepth;
        screen_data.width = screen.width;

        this.results.screen = screen_data;
        return this.results.screen;
    },
    getBrowserLanguage: function () {
        if (!this.results.browser_language) {
            this.results.browser_language = navigator.language;
        }

        return this.results.browser_language;
    },
    getTimeZone: function () {
        if (!this.results.timezone) {
            this.results.timezone = new Date().getTimezoneOffset();
        }
        return this.results.timezone
    },
    getClientGeo: function () {

        if (this.results.geo_data) {
            return Promise.resolve(this.results.geo_data);
        }

        return new Promise(function (resolve, reject) {
            $.getJSON('https://freegeoip.net/json/', function (response) {
                var geo_data = {};

                geo_data.ip = response.ip;
                geo_data.country = response.country_name;
                geo_data.city = response.city;
                geo_data.region = response.region_name;
                geo_data.region_code = response.region_code;
                geo_data.code = response.zip_code;
                geo_data.long = response.longitude;
                geo_data.lat = response.latitude;
                geo_data.timezone = response.time_zone;

                this.results.geo_data = geo_data;
                resolve(this.results.geo_data);
            }.bind(this));
        }.bind(this));
    },
    getAvailablePlugins: function () {

        if (this.results.plugins) {
            return this.results.plugins;
        }

        var plugins = {
            total: 0,
            list: []
        };

        var x = navigator.plugins.length;
        plugins.total = x;
        for (var i = 0; i < x; i++) {
            plugins.list.push(navigator.plugins[i].name);
        }

        this.results.plugins = plugins;

        return this.results.plugins;
    },
    getBrowserVersion: function () {
        this.results.browser_version = navigator.appVersion;
        return this.results.browser_version;
    },
    getBrowserData: function () {

        if (this.results.browser) {
            return this.results.browser;
        }

        var ua = navigator.userAgent, tem,
            M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if (/trident/i.test(M[1])) {
            tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
            return 'IE ' + (tem[1] || '');
        }
        if (M[1] === 'Chrome') {
            tem = ua.match(/\b(OPR|Edge)\/(\d+)/);
            if (tem != null) return tem.slice(1).join(' ').replace('OPR', 'Opera');
        }
        M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
        if ((tem = ua.match(/version\/(\d+)/i)) != null) M.splice(1, 1, tem[1]);

        this.results.browser = M.join(' ');
        return this.results.browser;
    },
    getGoogleAnalytics: function () {
        var url_string = location.search;
        if (!url_string) {
            this.results.google_analytics = null;
            return false;
        }

        var request = this._parseQueryString(url_string);
        var result = {
            utm_source: null,
            utm_medium: null,
            utm_campaign: null,
            utm_term: null,
            utm_content: null
        };

        if (request.utm_source && request.utm_source.length > 0) {
            result.utm_source = request.utm_source;
        }

        if (request.utm_medium && request.utm_medium.length > 0) {
            result.utm_medium = request.utm_medium;
        }

        if (request.utm_campaign && request.utm_campaign.length > 0) {
            result.utm_campaign = request.utm_campaign;
        }

        if (request.utm_term && request.utm_term.length > 0) {
            result.utm_term = request.utm_term;
        }

        if (request.utm_content && request.utm_content.length > 0) {
            result.utm_content = request.utm_content;
        }

        var is_result_not_empty = result.utm_source || result.utm_medium || result.utm_campaign || result.utm_term || result.utm_content;

        if (is_result_not_empty) {
            return this.google_analytics = result;
        }

        return false;
    },
    setContainer: function (container) {
        this.props.container = container;
    },
    getScrollContainer: function () {

        var container = null;

        for (var i = 0; i < document.body.children.length; i++) {
            var element = document.body.children[i];
            var height = Math.max(element.scrollHeight, element.offsetHeight);
            if (window.screen.height < height) {
                container = element;
                break;
            }
        }

        var max_height = Math.max(document.body.scrollHeight, document.body.offsetHeight);
        if (window.screen.height < max_height) {
            container = window;
        }

        return container;
    },
    scrollListener: function () {
        var body = document.body;
        var doc = this.props.container;

        var scrollHeight = (body.scrollHeight) ? body.scrollHeight : 0;
        var offsetHeight = (body.offsetHeight) ? body.offsetHeight : 0;
        var docClientHeight = (doc.clientHeight) ? doc.clientHeight : 0;
        var docScrollHeight = (doc.scrollHeight) ? doc.scrollHeight : 0;
        var docOffsetHeight = (doc.offsetHeight) ? doc.offsetHeight : 0;

        var docHeight = Math.max(scrollHeight, offsetHeight, docClientHeight, docScrollHeight, docOffsetHeight);

        var left = (window.pageXOffset || doc.scrollLeft) - (doc.clientLeft || 0);
        var top = (window.pageYOffset || doc.scrollTop) - (doc.clientTop || 0);

        if (!top) {
            top = 0;
        }

        var intFrameHeight = window.innerHeight;

        this.results.current = (((intFrameHeight + top) * 100) / docHeight).toFixed(0);
        if (this.results.current * 1 > this.results.max * 1) {
            this.results.max = this.results.current;
        }


        if (this.timer) {
            clearTimeout(this.timer);
        }

        this.timer = setTimeout(function () {
            var event_data = {
                current: this.results.current,
                max: this.results.max
            };
            var event = new CustomEvent('ScrollAnalyzer:Finished', {"detail": event_data});
            window.dispatchEvent(event);
        }.bind(this), this.wait_for_move_in_sec)

    },
    tryChangeFocus: function () {

        if (this.timer) {
            clearTimeout(this.timer);
        }

        if (document.hasFocus()) {
            this.timer = setTimeout(this.tryChangeFocus.bind(this), this.check_focus_interval);
        } else {
            this.dispatchFocusEvent();
        }

    },
    tryToClose: function (event) {
        //check for close page

        this.dispatchWindowCloseEvent();

        if (this.props.confirm_on_close) {
            var confirmationMessage = "\o/";
            (event || window.event).returnValue = confirmationMessage; //Gecko + IE
            return confirmationMessage;
        }

        return true;
    },
    dispatchFocusEvent: function () {
        var event = new Event('ClosePageAnalyzer:FocusOut');
        window.dispatchEvent(event);
    },
    dispatchWindowCloseEvent: function () {
        var event = new Event('ClosePageAnalyzer:WindowClose');
        window.dispatchEvent(event);
    },
    _runFocusCheck: function () {
        this._stopFocusCheck();
        this.tryChangeFocus();
    },
    _stopFocusCheck: function () {
        if (this.check_focus_timer) {
            clearTimeout(this.check_focus_timer);
        }
    },
    _parseQueryString: function (query) {
        var vars = query.replace('?', '').split("&");
        var query_string = {};
        for (var i = 0; i < vars.length; i++) {
            var pair = vars[i].split("=");
            // If first entry with this name
            if (typeof query_string[pair[0]] === "undefined") {
                query_string[pair[0]] = decodeURIComponent(pair[1]);
                // If second entry with this name
            } else if (typeof query_string[pair[0]] === "string") {
                var arr = [query_string[pair[0]], decodeURIComponent(pair[1])];
                query_string[pair[0]] = arr;
                // If third or later entry with this name
            } else {
                query_string[pair[0]].push(decodeURIComponent(pair[1]));
            }
        }
        return query_string;
    },
    attachCloseListener: function () {
        window.addEventListener("beforeunload", this.tryToClose.bind(this));
        this._runFocusCheck();
    },
    detachCloseListener: function () {
        window.removeEventListener("beforeunload", this.tryToClose.bind(this));
        this._stopFocusCheck();
    },
    attachScrollListener: function () {
        this.props.container.addEventListener('scroll', this.scrollListener.bind(this));
        this.props.container.addEventListener('resize', this.scrollListener.bind(this));
    },
    detachScrollListerner: function () {
        this.props.container.removeEventListener('scroll', this.scrollListener.bind(this));
        this.props.container.removeEventListener('resize', this.scrollListener.bind(this));
    }
};

function uniqid(pr, en) {
    var pr = pr || '', en = en || false, result, us;

    var seed = function (s, w) {
        s = parseInt(s, 10).toString(16);
        return w < s.length ? s.slice(s.length - w) :
            (w > s.length) ? new Array(1 + (w - s.length)).join('0') + s : s;
    };

    result = pr + seed(parseInt(new Date().getTime() / 1000, 10), 8)
        + seed(Math.floor(Math.random() * 0x75bcd15) + 1, 5);

    if (en) result += (Math.random() * 10).toFixed(8).toString();

    return result;
}

