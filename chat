function loadScript(url, callback) {
    var script = document.createElement('script');
    script.type = 'text/javascript';
    if (script.readyState) {
        script.onreadystatechange = function () {
            if (script.readyState == 'loaded' || script.readyState == 'complete') {
                script.onreadystatechange = null;
                callback();
            }
        };
    } else {
        script.onload = function () {
            callback();
        };
    }

    script.src = url;
    document.getElementsByTagName('head')[0].appendChild(script);
    return;
}

send_ajax = 0;
if (typeof jQuery == 'undefined') loadScript('https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js', jQueryLoaded);
else jQueryLoaded();

function jQueryLoaded() {
    loadScript('https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.4.6/socket.io.min.js', sendAjax);
}

// GET chat_id from head tag.
//
// Example:
// <script type="text/javascript" data-chat-id='e445ff50b4cb43d64f55103143114813' src="//localhost/Pushli/chat"></script>
function getChatIdFromScriptTag() {

    var current_script = document.currentScript;

    if (!current_script.hasAttribute('data-chat-id')) {
        return false;
    }

    var app_id = document.currentScript.getAttribute('data-chat-id');

    if (!app_id) {
        return false;
    }

    return app_id;

}

function getPageUrl() {

    var url = window.location.href.toString().split(window.location.host);

    if (typeof url[1] != 'undefined') {
        return window.location.protocol + '//' + window.location.hostname + url[1];
    }

    return false;
}

//http://modernapp.loc/app/indexjsNotification.php
//https://embed.modernapp.co/jsNotification.php
//http://localhost/Pushli/jsNotification.php
function sendAjax() {
    jQuery.ajax({
        url: "https://embed.modernapp.co/jsNotification.php",
        data: {
            code: '2ceb2b29aafed4bf3beb5ff83f71f4d5',
            url: getPageUrl()
        },
        cache: false,
        dataType: "json",
        success: function (data) {
            window.ModernFacebookMessengerAppID = data.app_id;
            setStyle(data.app_id);
            //if (data.id && data.id != 0){
            fProceedPuChat(data);
            // }

        },
        error: function () {
            if (send_ajax == 0) sendAjax();
            send_ajax = 1;
        }
    });
}

var g_chat;

function facebookInit() {
    if (typeof window.fbAsyncInit == "undefined") {
        window.fbAsyncInit = function () {
            FB.init({
                appId: "1097357530295857",
                xfbml: true,
                version: 'v2.6'
            });

            detectOptin();
        };
        var fb = $('<div/>', {
            'id': 'fb-root'
        }).appendTo('body');

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    }
    else {
        detectOptin();
    }
}

function getReferral(chat, include_extra) {
    var ref_url = window.location.href;
    var device_type = chat.is_mobile_device ? 'Mobile' : 'Desktop';
    var os = navigator.platform;

    var ip_info = JSON.parse(chat.ip_info);

    var ref = {ip: ip_info.ip, ref_url: ref_url, prev_url: ip_info.referrer, country: ip_info.country_name, city: ip_info.city, lang: ip_info.language, chat_code: chat.code, device_type: device_type, os: os};

    if (include_extra) {
        if (chat.optin_data && chat.optin_data.type == 'ref-extra-data') {
            for (var prop in chat.optin_data) {
                ref[prop] = chat.optin_data[prop];
            }
        }
    }

    return btoa(JSON.stringify(ref));
}

function getOptinData() {
    var ref = getReferral(g_chat, false);
    var data = {optinId: g_chat.optin_sid, pageId: g_chat.fb_page_id, chatMsgId: g_chat.id, data: g_chat.optin_data, ctx: ref};
    return btoa(JSON.stringify(data));
}

function injectOptin() {
    g_chat.optin_name = 'fb-messenger-optin-' + g_chat.app_id;

    $('#' + g_chat.optin_name).remove();

    var optin = $('<div/>', {
        'id': g_chat.optin_name,
        'style': 'position: absolute; opacity: 1; cursor: pointer; top: 6px; z-index: 2147483649; height: 40px;overflow: hidden;',
    }).appendTo('.fb-messenger-widget-message-footer');
    optin.html(
        '<div id="fbsend">' +
        '<div class="fb-send-to-messenger"' +
        'messenger_app_id="1097357530295857"' +
        'page_id="' + g_chat.fb_page_id + '"' +
        'data-ref="' + getOptinData() + '"' +
        'color="blue"' +
        'size="xlarge">' +
        '</div>' +
        '</div>');
    FB.XFBML.parse();
}

function openMessengerChat() {
    window.open(g_chat.url, '_blank', "width=500,height=800,left=");
}

function detectOptin() {
    if (g_chat.optin_text) {
        var b = g_chat.optin_text.indexOf('[[');
        var e = g_chat.optin_text.indexOf(']]', b + 2);
        var data = JSON.parse(g_chat.optin_text.substring(b + 2, e));

        if (typeof data.type == 'undefined') {
            // we default it to send to messenger if no type specified
            data.type = 'send-to-messenger';
        }

        g_chat.optin_data = data;

        var sock = io.connect("https://socket.modernapp.co:9090", {secure: true});
        sock.emit("optin_callback", g_chat.optin_sid);
        sock.on("message", function (msg) {
            var b = msg.indexOf('{{');
            if (b > -1) {
                var e = msg.indexOf('}}', b + 2);
                var anchor_text = msg.substring(b + 2, e);
                msg = msg.substring(0, b) + '<a href="javascript:void" onclick="openMessengerChat();">' + anchor_text + '</a>' + msg.substring(e + 2, msg.length);
            }

            var content = $('#fb-messenger-widget-' + g_chat.app_id).find('.fb-messenger-widget-message-item');
            var text = $('<div/>', {'class': 'fb-messenger-widget-message-item-text'}).appendTo(content);
            $('<div/>', {'class': 'fb-messenger-widget-message-item-text-inner'}).html(msg).appendTo(text);
        });
    }
}

function fProceedPuChat(chat) {
    (function ($) {
        $(function () {
            //var chat = '';

            function changePage() {

                EventCallback('page', getPageUrl());

                jQuery.ajax({
                    url: "https://embed.modernapp.co/jsNotification.php",
                    data: {
                        code: '2ceb2b29aafed4bf3beb5ff83f71f4d5',
                        url: getPageUrl()
                    },
                    dataType: "json",
                    cache: false,
                    success: function (data) {
                        chat = data;
                        $('#fb-messenger-widget-' + chat.app_id).remove();
                        renderWidget();
                    },
                    error: function () {
                        if (send_ajax == 0) sendAjax();
                        send_ajax = 1;
                    }
                });
            }

            function init() {
                var mess = getCookie('render_message');
                mess++;
                setCookie('render_message', mess, 7);
                var hashDetection = new hashHandler();

                if (!getCookie('fb_messenger_hide')) {
                    setCookie('fb_messenger_hide', 0, 7);
                }
                $('#fb-messenger-widget-' + chat.app_id).remove();
                renderWidget();
                EventCallback('show', 0);
                EventCallback('page', getPageUrl());
                setupShowFirstMessageTrigger();

                window.addEventListener('ScrollAnalyzer:Finished', function (event) {
                    EventCallback('scroll', btoa(JSON.stringify(event)));
                });

                window.addEventListener('ClosePageAnalyzer:FocusOut', function (event) {
                    var obj = {
                        uri: getPageUrl(),
                        results: Analyzer.getResults(),
                    };
                    var data = (btoa(unescape(encodeURIComponent(JSON.stringify(obj)))));
                    EventCallback('focusout', data);
                });

                window.addEventListener('ClosePageAnalyzer:WindowClose', function (event) {
                    var obj = {
                        uri: getPageUrl(),
                        results: Analyzer.getResults(),
                    };
                    var data = (btoa(unescape(encodeURIComponent(JSON.stringify(obj)))));
                    EventCallback('dropoff', data);
                });

                // optin_text implies we had "[[...]]" in greeting_text
                // widget_type: 2 = fb page plugin, 3 = fb checkbox
                if (chat.optin_text || chat.widget_type == 2 || chat.widget_type == 3) {
                    g_chat = chat;
                    facebookInit();
                }

                if (!window.ModernWidgetStorage) {
                    window.ModernWidgetStorage = new Map();
                }

                var customer = getCookie('modern_customer');
                var customer_data = {};
                if (customer) {
                    customer_data = JSON.parse(customer)
                } else {
                    customer_data = {
                        customer_id: uniqid()
                    };
                    setCookie('modern_customer', JSON.stringify(customer_data));
                }

                window.ModernWidgetStorage.set(chat.app_id, customer_data);

                // Analyzer.setContainer(window);
                Analyzer.setContainer(Analyzer.getScrollContainer());
                Analyzer.attachScrollListener();

                Analyzer.getBrowserData();
                Analyzer.getAvailablePlugins();
                Analyzer.getBrowserLanguage();
                Analyzer.getBrowserVersion();
                Analyzer.getScreenData();
                Analyzer.getTimeZone();
                Analyzer.getGoogleAnalytics();

                Analyzer.getClientGeo().then(response => {
                    var data = (btoa(unescape(encodeURIComponent(JSON.stringify(Analyzer.getResults())))));
                    EventCallback('client_info', data);
                });

                Analyzer.attachCloseListener();

            }

            function setCookie(n, v, ex) {
                //ex is the number of days
                var d = new Date();
                d.setTime(d.getTime() + ex * 60 * 86400);
                document.cookie = n + "=" + v + "; expires=" + d.toUTCString();
            }

            function getCookie(n) {
                var name = n + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') c = c.substring(1);
                    if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
                }
                return false;
            }

            function hashHandler() {

                var oldURL = "";

                var detect = function checkURLchange(currentURL) {
                    if (currentURL != oldURL) {
                        oldURL = currentURL;
                        changePage();
                    }
                };

                oldURL = window.location.href;

                setInterval(function () {
                    detect(window.location.href);
                }, 500);
            }

            function clickChatAvatar(do_redirect) {

                if (typeof(do_redirect) == 'undefined') {
                    do_redirect = true;
                }

                if (chat.is_mobile_device == true) {
                    EventCallback('click', 0);
                    return openFBChat(chat.url);
                }

                if (!chat.id) {
                    EventCallback('click', 0);
                    return openFBChat(chat.url);
                }

                if ($('#fb-messenger-widget-' + chat.app_id).hasClass('short')) {
                    if (!chat.fb_page_id || !chat.id || chat.is_mobile_device) {
                        return false;
                    }
                    renderWidget('full');
                    setCookie('fb_messenger_hide', 0, 100);
                    $('#fb-messenger-widget-' + chat.app_id + ' .fb-messenger-widget-close-button').show();
                } else {
                    renderWidget('short');
                    //if (do_redirect){
                    //    openFBChat(chat.url);
                    //}

                }

            }

            function close() {
                setCookie('fb_messenger_hide', 1, 7);
                renderWidget('short');
                //$('#fb-messenger-widget-'+chat.app_id+' .fb-messenger-widget-close-button').hide();
            }

            function hideBubbleAfter(time) {
                var hideTime = 2000;
                if (time) hideTime = time;

                clearTimeout($(this).data('disappearTimer'));
                var disappearTimer = setTimeout(function () {
                    renderWidget('');
                }, hideTime);
                $(this).data('disappearTimer', disappearTimer);
            }

            function DispalayPosition(type) {
                var str = '';
                if (chat.display_position == 0) {
                    str = ' bottom_right';
                } else {
                    str = ' bottom_left';
                    //$('.fb-messenger-widget-content').insertAfter($('.fb-messenger-widget-footer'));
                }

                if (type == 1) {
                    return str;
                }
            }

            function renderWidget(display_type) {

                if (getCookie('fb_messenger_shown') <= "1" || getCookie(chat.app_id + '_shown') == "1") {
                    chat.new_message_count = 0;
                } else {
                    chat.new_message_count = 1;
                }
                if (typeof(display_type) == 'undefined') {

                    display_type = '';

                    if (!chat.fb_page_id || !chat.id || getCookie('fb_messenger_hide') || chat.is_mobile_device) {
                        display_type = 'short';
                    }

                }

                setCookie('fb_messenger_shown', 1, 7);

                if ($('#fb-messenger-widget-' + chat.app_id).length) {

                    $('#fb-messenger-widget-' + chat.app_id).removeClass('short').removeClass('full');
                    $('#fb-messenger-widget-' + chat.app_id).addClass(display_type);

                    var widget_content = $('#fb-messenger-widget-' + chat.app_id + ' .fb-messenger-widget-content');

                    var widget_footer_block = $('#fb-messenger-widget-' + chat.app_id + ' .fb-messenger-widget-simple');
                    renderMessage(widget_content, display_type);
                } else {
                    display_type = display_type + DispalayPosition(1);
                    var widget = $('<div/>', {
                        'id': 'fb-messenger-widget-' + chat.app_id,
                        'z-index': '2147483646',
                        'class': display_type,
                        hover: function () {
                            EventCallback('hover', 0);
                            clearTimeout($(this).data('disappearTimer'));
                        },
                        mouseenter: function () {
                            clearTimeout($(this).data('mouseTimer'));
                            clearTimeout($(this).data('disappearTimer'));
                        },
                        mouseleave: function () {
                            if (getCookie('fb_messenger_hide') == "1") {
                                var timer = setTimeout(function () {
                                    renderWidget('short');

                                }, 500);
                                $(this).data('mouseTimer', timer);
                            }
                        }
                    }).appendTo('body');

                    var widget_container = $('<div/>', {'class': 'fb-messenger-widget-container'}).appendTo(widget);

                    var widget_header = $('<div/>', {'class': 'fb-messenger-widget-header'}).appendTo(widget_container);
                    var widget_content = $('<div/>', {'class': 'fb-messenger-widget-content'}).appendTo(widget_container);
                    var widget_footer = $('<div/>', {'class': 'fb-messenger-widget-footer'}).appendTo(widget_container);

                    /*var widget_close_button = $('<div/>', {
                     class : 'fb-messenger-widget-close-button',
                     click : function(e){
                     e.preventDefault();
                     close();
                     }
                     }).text('x').appendTo(widget_header);*/

                    var widget_footer_block = $('<div/>', {
                        class: 'fb-messenger-widget-simple',
                        click: function (e) {
                            e.preventDefault();
                            clickChatAvatar();
                        },
                        mouseenter: function () {
                            //if ($('#fb-messenger-widget-'+chat.app_id).hasClass('short')){
                            clearTimeout($(this).data('disappearTimer'));
                            if (!chat.fb_page_id) {
                                return false;
                            }
                            if (!chat.id) {
                                return false;
                            }
                            setTimeout(function () {
                                renderWidget('full');
                            }, 100);
                            //}
                        }
                    }).appendTo(widget_footer);

                    var widget_footer_block_img = $('<img/>', {'class': 'fb-messenger-widget-simple-image scalehover', 'src': chat.profile_picture}).appendTo(widget_footer_block);
                    // var widget_footer_block_img = $('<div/>', {'class': 'fb-messenger-widget-simple-image scalehover', 'style': 'background-color: ' + chat.header_style}).appendTo(widget_footer_block);
                    // var widget_footer_block_fb_logo = $('<span/>', {'class': 'fb-messenger-widget-simple-fb-logo'}).appendTo(widget_footer_block_img);
                    DispalayPosition(0);
                    // var widget_footer_block_img = $('<img/>', {'class': 'fb-messenger-widget-simple-image', 'src': chat.fb_page_profile_picture}).appendTo(widget_footer_block);
                    //var widget_footer_block_img = $('<img/>', {'class' : 'fb-messenger-widget-simple-image', 'src' : 'https://assets.modernapp.co/assets/images/fb-messenger-icon.png'}).appendTo(widget_footer_block);

                }

                if (chat.input_main_text != '') {
                    renderFooterBadge(widget_footer_block);
                }


                //renderMessage(widget_content, display_type);

            }

            function playSound() {
                var audio = new Audio('https://assets.modernapp.co/assets/audio/chat-sound.mp3');
                audio.play();
            }

            function renderFooterBadge(widget_footer_block) {

                widget_footer_block.find('.fb-messenger-widget-badge').remove();

                var widget_footer_block_badge = $('<span/>', {'class': 'fb-messenger-widget-badge scalehover'});

                if (chat.new_message_count <= 0) {
                    widget_footer_block_badge.addClass('empty').css('background-image', 'url(https://assets.modernapp.co/assets/images/fb-messenger-icon.png)');
                } else {
                    widget_footer_block_badge.text(chat.new_message_count).addClass('state-appear');
                }

                widget_footer_block_badge.appendTo(widget_footer_block);

                // widget_footer_block.find('.fb-messenger-widget-badge').remove();
                //
                // var widget_footer_block_badge = $('<span/>', {'class': 'fb-messenger-widget-badge scalehover'});
                //
                // if (chat.new_message_count <= 0) {
                //     // widget_footer_block_badge.addClass('empty').css('background-image', 'url(https://assets.modernapp.co/assets/images/fb-messenger-icon.png)');
                //     widget_footer_block_badge.addClass('empty');
                //     if (chat.profile_picture) {
                //         widget_footer_block_badge.css('background-image', 'url(' + chat.profile_picture + ')');
                //     } else {
                //         widget_footer_block_badge.css('background-image', 'url(' + chat.fb_page_profile_picture + ')');
                //     }
                // } else {
                //     widget_footer_block_badge.text(chat.new_message_count).addClass('state-appear');
                // }
                //
                // widget_footer_block_badge.appendTo(widget_footer_block);
            }

            function renderMessage(widget_content, display_type) {
                if (display_type == $(this).data('messageStatus'))
                    return;

                widget_content.empty();

                if (display_type == '') {
                    $(this).data('messageStatus', 'hidden');
                    return;
                }


                var widget_content_message_container = $('<div/>', {'class': 'fb-messenger-widget-content-message-container'}).appendTo(widget_content);
                var widget_content_message = $('<div/>', {'class': 'fb-messenger-widget-message'});


                if (display_type == 'short') {
                    $('.fb-messenger-widget-footer').css({'display': 'table-cell'});
                    renderShortMessage(widget_content_message);
                    $(this).data('messageStatus', 'short');
                    hideBubbleAfter(5000);
                }
                else if (display_type == 'typing') {
                    renderShortMessage(widget_content_message, '...');
                    $(this).data('messageStatus', 'typing');
                }

                widget_content_message.appendTo(widget_content_message_container);

                if (display_type == 'full') {

                    clearTimeout($(this).data('disappearTimer'));
                    $(this).data('messageStatus', 'full');
                    $('.fb-messenger-widget-footer').css({'display': 'none'});

                    if (chat.widget_type == 2) {

                        widget_content_message.appendTo(widget_content_message_container);
                        renderFbPlugin(widget_content_message);

                    } else {

                        renderFullMessage(widget_content_message);
                        var widget_message_arrow = $('<div/>', {'class': 'fb-messenger-widget-message-arrow-bottom'}).appendTo(widget_content_message_container);
                    }

                }

                if (chat.widget_type == 3 || chat.widget_type == 2) {
                    FB.XFBML.parse();
                }

                widget_content_message_container.addClass('state-appear');

            }

            function renderFbPlugin(widget_content_message) {

                var widget_content_message_header = $('<div/>', {'class': 'fb-messenger-widget-message-header', 'style': 'background-color: ' + chat.header_style}).appendTo(widget_content_message);
                var widget_content_message_header_icon = $('<img/>', {'class': 'fb-messenger-widget-message-header-icon'});

                if (chat.profile_picture) {
                    widget_content_message_header_icon.attr('src', chat.profile_picture);
                } else {
                    widget_content_message_header_icon.attr('src', chat.fb_page_profile_picture);
                }
                widget_content_message_header_icon.appendTo(widget_content_message_header);

                if (chat.header_background) {
                    var widget_content_message_header_overlay = $('<div/>', {
                        'class': 'fb-messenger-widget-message-header-overlay',
                        'style': 'opacity: 0; background-image: url(' + chat.header_background
                    }).appendTo(widget_content_message_header);
                }

                var widget_close_button = $('<div/>', {
                    class: 'fb-messenger-widget-close-button', click: function (e) {
                        e.preventDefault();
                        close();
                    }
                }).text('x').appendTo(widget_content_message_header);

                var widget_content_message_header_account_name = $('<span/>', {'class': 'fb-messenger-widget-account-name', 'style': 'color: ' + chat.header_text_color}).appendTo(widget_content_message_header);
                var widget_content_message_header_account_name_text = $('<span/>', {'class': 'fb-messenger-widget-account-name-text'}).text(chat.business_name).appendTo(widget_content_message_header_account_name);

                if (chat.business_name != '') {
                    var widget_content_message_header_account_name_secondary = $('<span/>', {'class': 'fb-messenger-widget-account-secondary-name'}).text(' via Messenger').appendTo(widget_content_message_header_account_name);
                }

                var widget_content_message_content = $('<div/>', {'class': 'fb-messenger-widget-message-content'}).appendTo(widget_content_message);

                var widget_content_message_content_item = $('<div/>', {'class': 'fb-messenger-widget-message-item'}).appendTo(widget_content_message_content);

                widget_content_message_content_item.html('<div id="fb-root"></div><div class="fb-page state-appear" data-small-header="true" data-hide-cover="true" style="width:300px" data-href="' + chat.fb_page_url + '" data-tabs="messages" data-height="240" data-width="280" data-small-header="false"  data-hide-cover="false" data-show-facepile="true"><blockquote cite="' + chat.fb_page_url + '" class="fb-xfbml-parse-ignore"><a href="' + chat.fb_page_name + '">' + chat.fb_page_name + '</a></blockquote></div>');


                var widget_content_message_footer = $('<div/>', {
                    class: 'fb-messenger-widget-message-footer'
                }).appendTo(widget_content_message);

                var widget_content_message_footer_inner = $('<div/>', {
                    'class': 'fb-messenger-widget-message-footer-inner',
                    click: function (e) {
                        e.preventDefault();
                        EventCallback('click', 0);
                        openFBChat(chat.url);
                    }
                }).appendTo(widget_content_message_footer);

                if (chat.avoid_branding == 0) {
                    var ref_url = window.location.href;
                    device_type = chat.is_mobile_device ? 'Mobile' : 'Desktop';
                    var widget_content_message_content_branding = $('<div/>', {'class': 'fb-messenger-widget-branding'}).text('we run on ').appendTo(widget_content_message_footer_inner);
                    var widget_content_message_content_branding_link = $('<a/>', {
                        'href': 'https://modernapp.co?utm_campaign=widget&utm_medium=' + ref_url + '&utm_source=modernbranding&utm_device=' + device_type,
                        'target': '_blank'
                    }).text('Modern').appendTo(widget_content_message_content_branding);
                }
            }

            function renderShortMessage(widget_content_message, text) {
                var version = jQuery.fn.jquery;
                version = Number(version.substring(0, 3));
                if (version >= 1.3 && version <= 1.4) {
                    widget_content_message.live('click', function (e) {
                        e.preventDefault();
                        //EventCallback('click',0);
                        openFBChat(chat.url);
                    });
                } else {
                    widget_content_message.on('click', function (e) {
                        e.preventDefault();
                        //EventCallback('click',0);
                        openFBChat(chat.url);
                    });
                }

                switch (chat.button_style_type * 1) {
                    case 2:
                        widget_content_message.addClass('fb-messenger-widget-message-blue');
                        break;
                }

                var message_text = false;

                if (!chat.id && !chat.is_mobile_device) {
                    message_text = (text ? text : chat.short_text);
                } else {
                    message_text = (text ? text : chat.input_main_text_short);
                }

                if (!message_text) {
                    message_text = 'click to chat';
                }

                widget_content_message.text(message_text);

                if (chat.input_main_text_short == '') {
                    widget_content_message.addClass('fb-messenger-widget-hidden');
                }

                var widget_content_message_content_item_avatar_img = $('<img/>', {
                        class: 'fb-messenger-widget-account-picture'
                    }
                )

                switch (chat.button_style_type * 1) {
                    case 2:
                        widget_content_message_content_item_avatar_img.attr('src', 'data:image/png;base64,');
                        break;
                    default:
                        widget_content_message_content_item_avatar_img.attr('src', 'data:image/png;base64,');
                        break;
                }

                widget_content_message_content_item_avatar_img.appendTo(widget_content_message);
                DispalayPosition(0);

            }

            function renderFullMessage(widget_content_message) {

                var widget_content_message_header = $('<div/>', {'class': 'fb-messenger-widget-message-header', 'style': 'background-color: ' + chat.header_style}).appendTo(widget_content_message);
                var widget_content_message_header_icon = $('<img/>', {'class': 'fb-messenger-widget-message-header-icon'});

                if (chat.profile_picture) {
                    widget_content_message_header_icon.attr('src', chat.profile_picture);
                } else {
                    widget_content_message_header_icon.attr('src', chat.fb_page_profile_picture);
                }
                widget_content_message_header_icon.appendTo(widget_content_message_header);

                if (chat.header_background) {
                    var widget_content_message_header_overlay = $('<div/>', {
                        'class': 'fb-messenger-widget-message-header-overlay',
                        'style': 'opacity: 0; background-image: url(' + chat.header_background
                    }).appendTo(widget_content_message_header);
                }

                var widget_close_button = $('<div/>', {
                    class: 'fb-messenger-widget-close-button', click: function (e) {
                        e.preventDefault();
                        close();
                    }
                }).text('x').appendTo(widget_content_message_header);

                var widget_content_message_header_account_name = $('<span/>', {'class': 'fb-messenger-widget-account-name', 'style': 'color: ' + chat.header_text_color}).appendTo(widget_content_message_header);
                var widget_content_message_header_account_name_text = $('<span/>', {'class': 'fb-messenger-widget-account-name-text'}).text(chat.business_name).appendTo(widget_content_message_header_account_name);

                if (chat.business_name != '') {
                    var widget_content_message_header_account_name_secondary = $('<span/>', {'class': 'fb-messenger-widget-account-secondary-name'}).text(' via Messenger').appendTo(widget_content_message_header_account_name);
                }

                var widget_content_message_content = $('<div/>', {'class': 'fb-messenger-widget-message-content'}).appendTo(widget_content_message);

                if (chat.widget_type && ( chat.widget_type == 1 || chat.widget_type == 3 )) {
                    var widget_content_message_content_time = $('<div/>', {'class': 'fb-messenger-widget-message-time'}).text('today').appendTo(widget_content_message_content);
                }

                var widget_content_message_content_item = $('<div/>', {'class': 'fb-messenger-widget-message-item'}).appendTo(widget_content_message_content);
                var widget_content_message_content_item_avatar = $('<div/>', {'class': 'fb-messenger-widget-message-item-avatar'}).appendTo(widget_content_message_content_item);
                var widget_content_message_content_item_avatar_img = $('<img/>', {
                    class: 'fb-messenger-widget-account-picture',
                    src: chat.profile_picture
                }).appendTo(widget_content_message_content_item_avatar);

                // var widget_content_message_content_item_avatar_badge = $('<span/>', {'class': 'fb-messenger-widget-type'}).appendTo(widget_content_message_content_item_avatar);

                var widget_content_message_content_item_text = $('<div/>', {'class': 'fb-messenger-widget-message-item-text'}).appendTo(widget_content_message_content_item);
                var widget_content_message_content_item_text_inner = $('<div/>', {'class': 'fb-messenger-widget-message-item-text-inner'}).appendTo(widget_content_message_content_item_text);
                var widget_content_message_content_item_text_container = $('<div/>', {'class': 'fb-messenger-widget-message-item-text-container'}).html(chat.greeting_text).appendTo(widget_content_message_content_item_text_inner);

                if (chat.greeting_image) {
                    var widget_content_message_content_item_image = $('<div/>', {'class': 'fb-messenger-widget-message-item-image'}).appendTo(widget_content_message_content_item_text);
                    var widget_content_greeting_image_img = $('<img/>', {
                        src: chat.greeting_image,
                    }).appendTo(widget_content_message_content_item_image);
                }

                var widget_content_message_footer = $('<div/>', {
                    class: 'fb-messenger-widget-message-footer'
                }).appendTo(widget_content_message);

                var widget_content_message_footer_inner = $('<div/>', {
                    'class': 'fb-messenger-widget-message-footer-inner',
                    click: function (e) {
                        e.preventDefault();
                        EventCallback('click', 0);
                        openFBChat(chat.url);
                    }
                }).appendTo(widget_content_message_footer);

                var widget_content_message_footer_inner_button = $('<div/>', {'class': 'fb-messenger-widget-message-button'});

                if (chat.input_main_text == '') {
                    widget_content_message_footer.addClass('fb-messenger-widget-hidden');
                    widget_content_message.addClass('fb-messenger-widget-hidden');
                    //widget_content_message_content_item_avatar_badge.remove();
                }

                widget_content_message_footer_inner_button.text(chat.input_main_text);

                if (chat.optin_data && chat.optin_data.type == 'send-to-messenger') {
                    injectOptin();
                }

                switch (chat.button_style_type * 1) {
                    case 2:
                        widget_content_message_footer_inner_button.addClass('fb-messenger-widget-message-button-blue');
                        break;
                }

                widget_content_message_footer_inner_button.appendTo(widget_content_message_footer_inner);

                if (chat.widget_type == 3) {
                    var d = new Date();
                    chat.user_ref = d.getTime();
                    var hostname_origin = window.location.protocol + '//' + window.location.hostname;
                    var widget_content_message_footer_checkbox = $('<div/>', {'class': 'fb-messenger-widget-message-checkbox'});

                    widget_content_message_footer_checkbox.html('<div class="fb-messenger-checkbox" origin="' + hostname_origin + '" page_id="' + chat.fb_page_id + '" messenger_app_id="1097357530295857" user_ref="' + chat.user_ref + '"  prechecked="true" allow_login="true" size="small"></div>');
                    widget_content_message_footer_checkbox.appendTo(widget_content_message_footer_inner);
                }

                if (chat.avoid_branding == 0) {
                    var ref_url = window.location.href;
                    device_type = chat.is_mobile_device ? 'Mobile' : 'Desktop';
                    var widget_content_message_content_branding = $('<div/>', {'class': 'fb-messenger-widget-branding'}).html('<a style="color: #e1e1e1; text-decoration: none;" href="https://modernapp.co">We`re <span style="top: -5px;" class="fb-messenger-widget-branding-icon"></span> by </a>').appendTo(widget_content_message_footer_inner);
                    var widget_content_message_content_branding_link = $('<a/>', {
                        'href': 'https://modernapp.co?utm_campaign=widget&utm_medium=' + ref_url + '&utm_source=modernbranding&utm_device=' + device_type,
                        'target': '_blank'
                    }).text('Modern').appendTo(widget_content_message_content_branding);
                }

                //var widget_content_message_footer_inner_via_badge = $('<span/>').text(chat.input_secondary_text).appendTo(widget_content_message_footer_inner_via);

            }

            function setupShowFirstMessageTrigger() {

                if (chat.timeout_pregreeting > 0) {
                    var mess = getCookie('render_message');
                    if (getCookie('fb_messenger_hide') != "1" && ( mess <= 1 && chat.showon == 1) || chat.showon == 0) {
                        if (!$('#fb-messenger-widget-' + chat.app_id).hasClass('full')) {
                            setTimeout(function () {
                                renderWidget('short', '...');
                                if (!chat.is_mobile_device) {
                                    //  setTimeout(function () {
                                    renderWidget('full');
                                    // Only play sound if hasn't played before for this message
                                    if (getCookie(chat.app_id + '_shown') != "1") {
                                        playSound();
                                    }
                                    setCookie(chat.app_id + '_shown', 1, 7);
                                    //}, 2000);
                                }
                            }, chat.timeout_pregreeting * 1000);
                        }
                    }
                }
                else if (chat.about_to_leave) {
                    $(document).one('mouseleave', function () {
                        renderWidget('full');
                        playSound();
                    });
                }

            }

            function openFBChat(url) {

                if (!url) {
                    return false;
                }

                //fix when fb messenger not opened by username
                url = 'https://m.me/' + chat.fb_page_id;

                window.open(url + '?ref=' + getReferral(chat, true), '_blank', "width=500,height=800,left=" + screenwidth);
                var domain = window.location.host;
                var url = window.location.pathname;
                sendUrl(domain, url);
                var screenwidth = screen.width - 500;
                if (chat.widget_type == 3) {
                    FB.AppEvents.logEvent('MessengerCheckboxUserConfirmation', null, {
                        'app_id': '1097357530295857',
                        'page_id': chat.fb_page_id,
                        'ref': getOptinData(),
                        'user_ref': chat.user_ref
                    });
                }


            }

            function sendUrl(domain, url) {
                jQuery.ajax({
                    url: "https://embed.modernapp.co/click_url.php",
                    data: {
                        'domain': domain,
                        'url': url,
                        'code': chat.code
                    },
                    success: function (data) {
                        console.log(data);
                    },
                    error: function (data) {
                    }
                });
            }

            function EventCallback(ev, p) {

                if (chat.trackev == 1) {

                    var is_mobile = 0;
                    if (chat.is_mobile_device && chat.is_mobile_device == true) {
                        is_mobile = 1;
                    }

                    var customer = null;
                    if (window.ModernWidgetStorage) {
                        customer = window.ModernWidgetStorage.get(chat.app_id);
                    }

                    if (customer) {
                        $('<img/>').attr('src', chat.abs_path + 'evchat?c_id=' + customer.customer_id + '&m_id=' + chat.id + '&code=' + chat.code + '&m=' + is_mobile + '&ev=' + ev + '&p=' + p);
                    } else {
                        $('<img/>').attr('src', chat.abs_path + 'evchat?m_id=' + chat.id + '&code=' + chat.code + '&m=' + is_mobile + '&ev=' + ev + '&p=' + p);
                    }

                }
            }

            init();

        });
    })(jQuery);

}
